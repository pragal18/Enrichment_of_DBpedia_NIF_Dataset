""" ;
    nif:beginIndex "745" ;
    nif:endIndex "754" ;
    nif:referenceContext <http://dbpedia.org/resource/Apiales?dbpv=2016-10&nif=context> .

<http://dbpedia.org/resource/Apiales?dbpv=2016-10&nif=sentence_offset_754_913> a nif:Sentence ;
    nif:anchorOf "Under the Cronquist system, only the Apiaceae and Araliaceae were included here, and the restricted order was placed among the rosids rather than the asterids." ;
    nif:beginIndex "754" ;
    nif:endIndex "913" ;
    nif:referenceContext <http://dbpedia.org/resource/Apiales?dbpv=2016-10&nif=context> .

<http://dbpedia.org/resource/Apiales?dbpv=2016-10&nif=sentence_offset_914_1021> a nif:Sentence ;

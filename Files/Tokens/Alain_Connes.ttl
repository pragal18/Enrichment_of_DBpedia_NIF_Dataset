    nif:beginIndex "706" ;
    nif:endIndex "734" ;
    nif:referenceContext <http://dbpedia.org/resource/Alain_Connes?dbpv=2016-10&nif=context> .

<http://dbpedia.org/resource/Alain_Connes?dbpv=2016-10&nif=sentence_offset_735_880> a nif:Sentence ;
    nif:anchorOf "Connes has applied his work in areas of mathematics and theoretical physics, including number theory, differential geometry and particle physics." ;
    nif:beginIndex "735" ;
    nif:endIndex "880" ;
    nif:referenceContext <http://dbpedia.org/resource/Alain_Connes?dbpv=2016-10&nif=context> .


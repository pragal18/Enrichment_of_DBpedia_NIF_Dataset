    nif:beginIndex "160" ;
    nif:endIndex "190" ;
    nif:referenceContext <http://dbpedia.org/resource/Actrius?dbpv=2016-10&nif=context> .

<http://dbpedia.org/resource/Actrius?dbpv=2016-10&nif=sentence_offset_1620_1841> a nif:Sentence ;
